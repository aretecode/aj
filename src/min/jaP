function jaP(e){var t=window.ActiveXObject?new ActiveXObject("Microsoft.XMLHttp"):window.XMLHttpRequest?new XMLHttpRequest:!1;t.onreadystatechange=function(){4==t.readyState&&(200==t.status?e.success(t.responseText):e.error(t),e.done instanceof Function&&e.done(t))},t.open("POST",e.url,!0),t.setRequestHeader("X-Requested-With",e.requestedWith||"XMLHttpRequest"),t.setRequestHeader("Content-type",e.contentType||"application/x-www-form-urlencoded"),t.send(e.data)}
